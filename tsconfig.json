{
    "compilerOptions": {
      "outDir": "dist",
      "sourceMap": true,
      "moduleResolution": "node",
      "noImplicitAny": true,
      "experimentalDecorators": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "baseUrl": "./",
      "paths": {
        "@components/*": ["src/components/*"],
        "@utils/*": ["src/utils/*"],
        "@models/*": ["src/models/*"]
      },
      "typeRoots": [
        "src/models",
        "./node_modules/@types/"
      ]

    }
}